<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Loginregnardos"
        tools:targetApi="31">
        <activity
            android:name=".CardActivity"
            android:exported="false"
            android:label="@string/title_activity_card"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".ColumnActivity"
            android:exported="false"
            android:label="@string/title_activity_column"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".Slmplelist"
            android:exported="false"
            android:label="@string/title_activity_slmplelist"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".CalculatorActivity"
            android:exported="false"
            android:label="@string/title_activity_calculator"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".IntentActivity"
            android:exported="false"
            android:label="@string/title_activity_workgiven"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".ui.theme.workgiven"
            android:exported="false"
            android:label="@string/title_activity_workgiven"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".RegistrationActivitynared"
            android:exported="false"
            android:label="@string/title_activity_registration_activitynared"
            android:theme="@style/Theme.Loginregnardos" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Loginregnardos">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>